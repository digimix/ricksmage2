<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no"/>

	    <!-- Add external resources -->
	    <script src="https://use.typekit.net/ohs4hcy.js" src_type="url"></script>

		<!--Font Awesome-->
		<css src="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css" src_type="url" />

		<block type="core/text" name="typekit_font">
		    <action method="setText">
		        <text>
		            <![CDATA[<script>try{Typekit.load({ async: true });}catch(e){}</script>]]>
		        </text>
		    </action>
		</block>
    </head>
</page>